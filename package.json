{
  "name": "angular-highcharts",
  "version": "0.7.2",
  "description": "Highcharts Directive for Angular2",
  "main": "dist/bundles/angular-highcharts.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf index.js index.d.ts src/**/*.js src/**/*.d.ts",
    "build": "tsc && rollup -c",
    "lint": "tslint src/**/*.ts",
    "test": "npm run lint"
  },
  "author": "Felix Itzenplitz",
  "repository": {
    "type": "git",
    "url": "https://github.com/cebor/angular-highcharts.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "highcharts",
    "chart",
    "directive",
    "component"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/core": "2.1.2",
    "@types/highcharts": "^4.2.39",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rxjs": "5.0.0-beta.12",
    "tslint": "^4.0.2",
    "typescript": "^2.0.8",
    "zone.js": "^0.7.1"
  }
}
